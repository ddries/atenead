{
    "appId": "dev.driescode.atenead",
    "asar": true,
    "productName": "Atenead",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [ "x64" ]
        }
      ],
      "artifactName": "${name}.${os}.${ext}"
    },
    "mac": {
      "target": [ "dmg", "zip" ]
    },
    "nsis": {
      "perMachine": false,
      "deleteAppDataOnUninstall": true
    },

    "beforePack": "./beforePack.js"
  }
  